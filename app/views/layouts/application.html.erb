<!DOCTYPE html>
<html>
  <head>
    <title>TodoApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    <%= javascript_importmap_tags %>
  </head>

  <body>
    <%= link_to raw('<i class="fa-solid fa-house"></i>'), lists_path, class:'home-link' %>
    <%= link_to raw('<i class="fa-solid fa-arrow-right-from-bracket"></i>'), session_path, method: :delete, class: "logout-link" %>
    <%= yield %>
  </body>
</html>
